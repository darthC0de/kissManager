{
  "name": "kissmanager",
  "version": "1.0.0",
  "description": "Gerenciador de senhas",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darthc0de/kissManagerMobile.git"
  },
  "bugs": {
    "url": "https://github.com/darthc0de/kissManagerMobile/issues"
  },
  "homepage": "https://github.com/darthc0de/kissManagerMobile",
  "authors": [
    "Rodrigo Cordeiro <rodrigomendoncca@gmail.com> (https://rodcordeiro.com.br)",
    "Junior Alves",
    "Guilherme Pedroso",
    "Carolina Santos"
  ],

  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/picker": "^1.8.1",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "expo": "~42.0.1",
    "expo-linear-gradient": "~9.2.0",
    "expo-sqlite": "~9.2.1",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-flash-message": "^0.1.23",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-item-select": "^0.3.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-web": "^0.17.1",
    "react-native-web-hooks": "^3.0.1",
    "react-navigation": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@types/react": "16.14.14",
    "@types/react-native": "0.63.52",
    "typescript": "4.0.8"
  },
  "private": false,
  "renovate":{
    "branchPrefix": "updates/",
    "packageRules": [
      {
        "updateTypes": ["minor", "patch", "pin", "digest"],
        "automerge": true
      },{
        "updateTypes": ["major"],
        "automerge": false
      }
    ],
    "assignAutomerge":true,
    "assignees":[
      "rodcordeiro",
      "JuniorAlvess"
    ],
    "labels": ["dependencies"],
    "recreateClosed":true,
    "rebaseWhen":"conflicted",
    "vulnerabilityAlerts": {
      "labels": ["security"],
      "assignees": ["@rodcordeiro"]
    },
    "extends": [
      "config:base",
      ":dependencyDashboard",
      "github>whitesource/merge-confidence:beta"
    ]
  }
}
