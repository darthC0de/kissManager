{
  "expo": {
    "name": "kissManager",
    "slug": "kissManager",
    "version": "1.0.0",
    "description": "Gerenciador de senhas",
    "owner": "darthcode",
    "githubUrl": "https://github.com/darthc0de/kissManagerMobile",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#00587A",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00587A"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#A9F448"
      },
      "package": "com.darthcode.kissManager"
    },
    "notification": {
      "color": "#00587A"
    }
  }
}
